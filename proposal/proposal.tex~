\subsection{}

%This is an example algorithm
\begin{algorithmic}
  \Function{DoAThing}{$Parameter$}
  \For{$i \gets 1$ to $n$}
  \State $A \gets A+i$
  \If{$A<4$}
  \State PrintAThing
  \EndIf
  \EndFor
  \EndFunction
\end{algorithmic}

